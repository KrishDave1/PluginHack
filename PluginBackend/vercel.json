{
  "version": 2,
  "builds": [
    {
      "src": "PluginBackend/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.10" }
    },
    {
      "src": "./build-files.sh",
      "use": "@vercel/static-build",
      "config": { "distDir": "PluginBackend" }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "PluginBackend/wsgi.py"
    }
  ]
}
